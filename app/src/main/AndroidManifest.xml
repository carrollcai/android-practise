<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practise">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="测试">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".counter.CounterActivity" />
        <activity android:name=".recyclerview.LinearActivity" />
        <activity android:name=".recyclerview.RecyclerViewActivity" />
<!--        <activity android:name=".view.RecyclerViewActivity" />-->
        <activity android:name=".NewActivity" />
        <service android:name=".counter.TimeService">
            <intent-filter>
                <action android:name="TIME_CHANGE_ACTION"></action>
            </intent-filter>
        </service>
    </application>

</manifest>